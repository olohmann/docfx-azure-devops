{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "labs/**.md",
          "labs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**",
          "**/images/**",
          "**/img/**",
          "**/visio/**",
          "**/artifacts/**",
          "codesnippet/**",
          "langwordmapping/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appTitle": "Contoso Sample Documentation",
      "_appFooter": "<span>Copyright Â© Contoso <br>Build: <strong>#{Build.BuildNumber}#</strong> Build-Branch: <strong>#{Build.SourceBranchName}#</strong></span>",
      "_gitContribute": {
        "branch": "main"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "_custom_template"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
